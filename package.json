{
  "name": "nodegui-plugin-qtmultimedia",
  "version": "1.0.0",
  "description": "QtMultimedia plugin for NodeGui",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run build:addon",
    "build:addon": "cross-env CMAKE_BUILD_PARALLEL_LEVEL=8 cmake-js compile",
